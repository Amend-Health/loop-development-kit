{
  "name": "ldk-javascript-wp-default",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --entry core-js/fn/promise --entry ./index.ts --config ./node_modules/@oliveai/ldk/dist/webpack/config.js"
  },
  "author": "",
  "license": "ISC",
  "ldk": {
    "permissions": {
      "clipboard": {
        "reason": "To monitor the users clipboard"
      },
      "filesystem": {
        "pathGlobs": [
          {
            "value": "/some/path/something.txt",
            "reason": "To read the shared file"
          }
        ]
      },
      "network": {
        "urlDomains": [
          {
            "value": "*.google.com",
            "reason": "For Google searches"
          }
        ]
      },
      "window": {
        "reason": "To monitor users window activity"
      }
    }
  },
  "dependencies": {
    "@oliveai/ldk": "file:../..",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  },
  "devDependencies": {
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4"
  }
}
